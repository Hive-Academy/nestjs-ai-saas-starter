{
  "name": "@nestjs-ai-saas-starter/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "api": "npx nx run dev-brand-api:serve",
    "start": "npx nx run ai-saas-frontend:serve:development",
    "dev:services": "docker compose -f docker-compose.dev.yml up -d",
    "dev:stop": "docker compose -f docker-compose.dev.yml down",
    "dev:reset": "docker compose -f docker-compose.dev.yml down -v",
    "dev:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:prod": "docker compose -f docker-compose.yml up -d",
    "build:libs": "nx run-many -t build -p nestjs-chromadb,nestjs-neo4j,nestjs-langgraph",
    "publish:dry-run": "npm run build:libs && nx run-many -t publish --dry-run -p nestjs-chromadb,nestjs-neo4j,nestjs-langgraph",
    "publish:libs": "npm run build:libs && nx run-many -t publish -p nestjs-chromadb,nestjs-neo4j,nestjs-langgraph",
    "version:libs": "nx release version",
    "publish:release": "nx release publish",
    "docs:generate": "nx run-many -t docs -p nestjs-chromadb,nestjs-neo4j,nestjs-langgraph",
    "docs:serve": "npx http-server docs/api -p 8080 -o",
    "docs:build-site": "npm run docs:generate && node scripts/build-docs-site.js",
    "prepare": "husky install",
    "lint:fix": "nx run-many -t lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate:package-json": "node scripts/validate-package-json.js",
    "validate:types": "node scripts/typescript-fixes/validate-types.js",
    "validate:types:typescript": "node scripts/typescript-fixes/validate-types.js typescript",
    "validate:types:eslint": "node scripts/typescript-fixes/validate-types.js eslint",
    "validate:types:any": "node scripts/typescript-fixes/validate-types.js any-types",
    "validate:types:build": "node scripts/typescript-fixes/validate-types.js build",
    "apply:strict-config": "node scripts/typescript-fixes/apply-strict-config.js",
    "ci:validate": "node scripts/typescript-fixes/ci-validation.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "20.1.6",
    "@angular/animations": "20.1.6",
    "@angular/cdk": "20.1.6",
    "@angular/cli": "20.1.6",
    "@angular/common": "20.1.6",
    "@angular/core": "20.1.6",
    "@angular/forms": "20.1.6",
    "@angular/platform-browser": "20.1.6",
    "@angular/platform-browser-dynamic": "20.1.6",
    "@angular/router": "20.1.6",
    "@huggingface/inference": "^4.6.1",
    "@langchain/anthropic": "^0.3.26",
    "@langchain/core": "^0.3.68",
    "@langchain/google-genai": "^0.2.16",
    "@langchain/langgraph": "^0.4.3",
    "@langchain/langgraph-checkpoint": "^0.1.0",
    "@langchain/langgraph-checkpoint-sqlite": "^0.2.0",
    "@langchain/openai": "^0.6.7",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "axios": "^1.6.0",
    "chromadb": "^3.0.11",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cohere-ai": "^7.18.0",
    "langchain": "^0.3.30",
    "neo4j-driver": "^5.28.1",
    "openai": "^5.12.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.2",
    "zod": "^3.25.76",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/core": "~20.1.6",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@eslint/js": "^9.8.0",
    "@nestjs/axios": "4.0.1",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/angular": "21.4.1",
    "@nx/eslint": "21.4.1",
    "@nx/eslint-plugin": "21.4.1",
    "@nx/jest": "21.4.1",
    "@nx/js": "21.4.1",
    "@nx/nest": "21.4.1",
    "@nx/node": "21.4.1",
    "@nx/rollup": "21.4.1",
    "@nx/web": "21.4.1",
    "@nx/webpack": "21.4.1",
    "@nx/workspace": "21.4.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/uuid": "^10.0.0",
    "chalk": "5.5.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "^9.0.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^15.0.0",
    "madge": "8.0.0",
    "nx": "21.4.1",
    "prettier": "^2.6.2",
    "rollup": "4.49.0",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "2.8.1",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^4.0.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "verdaccio": "^6.0.5",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "libs/langgraph-modules/*",
    "libs/demo/*",
    "libs/dev-brand/backend/*",
    "libs/core/*"
  ],
  "overrides": {
    "chalk": {
      "typescript": "$typescript"
    },
    "@angular-devkit/build-angular": {
      "jest": "$jest"
    },
    "@angular/compiler": "20.1.6",
    "@angular/compiler-cli": "20.1.6",
    "@angular/build": "20.1.6",
    "@swc/core": "~1.5.7"
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
