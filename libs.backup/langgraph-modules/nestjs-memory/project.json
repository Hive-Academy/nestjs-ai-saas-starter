{
  "name": "langgraph-modules/nestjs-memory",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/langgraph-modules/nestjs-memory/src",
  "projectType": "library",
  "tags": ["domain:langgraph", "type:data-access", "module:memory"],
  "// targets": "to see all targets run: nx show project langgraph-modules/nestjs-memory --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/langgraph-modules/nestjs-memory",
        "main": "libs/langgraph-modules/nestjs-memory/src/index.ts",
        "tsConfig": "libs/langgraph-modules/nestjs-memory/tsconfig.lib.json",
        "assets": [
          "libs/langgraph-modules/nestjs-memory/*.md",
          {
            "input": "./libs/langgraph-modules/nestjs-memory",
            "glob": "**/*.!(ts)",
            "output": "./"
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{options.outputFile}"],
      "options": {
        "jestConfig": "libs/langgraph-modules/nestjs-memory/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "coverage": true
        }
      }
    }
  }
}
