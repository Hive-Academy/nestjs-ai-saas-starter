{
  "name": "@hive-academy/nestjs-neo4j",
  "version": "0.0.1",
  "description": "NestJS integration for Neo4j graph database with comprehensive TypeScript support",
  "author": "Hive Academy Team",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "neo4j",
    "graph-database",
    "cypher",
    "relationships",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hive-academy/nestjs-ai-saas-starter.git",
    "directory": "libs/nestjs-neo4j"
  },
  "homepage": "https://github.com/hive-academy/nestjs-ai-saas-starter#readme",
  "bugs": {
    "url": "https://github.com/hive-academy/nestjs-ai-saas-starter/issues"
  },
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@langchain/anthropic": "^0.3.26",
    "@langchain/core": "^0.3.68",
    "@langchain/google-genai": "^0.2.16",
    "@langchain/langgraph": "^0.4.3",
    "@langchain/langgraph-checkpoint": "^0.1.0",
    "@langchain/langgraph-checkpoint-sqlite": "^0.2.0",
    "@langchain/openai": "^0.6.7",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "chromadb": "^3.0.11",
    "cohere-ai": "^7.18.0",
    "langchain": "^0.3.30",
    "neo4j-driver": "^5.28.1",
    "openai": "^5.12.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.2"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.0"
  },
  "nx": {
    "tags": [
      "domain:neo4j",
      "type:feature"
    ],
    "targets": {
      "build": {
        "executor": "@nx/rollup:rollup",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/nestjs-neo4j",
          "main": "libs/nestjs-neo4j/src/index.ts",
          "tsConfig": "libs/nestjs-neo4j/tsconfig.lib.json",
          "format": [
            "cjs",
            "esm"
          ],
          "assets": [
            {
              "input": "./libs/nestjs-neo4j",
              "glob": "*.md",
              "output": "./"
            },
            {
              "input": "./libs/nestjs-neo4j",
              "glob": "package.json",
              "output": "./"
            }
          ]
        }
      },
      "publish": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npm publish dist/libs/nestjs-neo4j --access public"
        },
        "dependsOn": [
          "build"
        ]
      },
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc --noEmit --project tsconfig.lib.json",
          "cwd": "libs/nestjs-neo4j"
        },
        "dependsOn": [
          "^typecheck"
        ],
        "cache": true
      },
      "docs": {
        "executor": "nx:run-commands",
        "options": {
          "command": "typedoc --options typedoc.json",
          "cwd": "libs/nestjs-neo4j"
        }
      }
    }
  }
}
