{
  "name": "devbrand-backend-data-access",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dev-brand/backend/data-access/src",
  "projectType": "library",
  "tags": ["scope:devbrand", "type:data-access", "platform:backend"],
  "// targets": "to see all targets run: nx show project devbrand-backend-data-access --web",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/dev-brand/backend/data-access",
        "main": "libs/dev-brand/backend/data-access/src/index.ts",
        "tsConfig": "libs/dev-brand/backend/data-access/tsconfig.lib.json",
        "rollupConfig": "libs/dev-brand/backend/data-access/rollup.config.js",
        "format": ["cjs", "esm"],
        "useLegacyTypescriptPlugin": false,
        "assets": [
          {
            "input": "./libs/dev-brand/backend/data-access",
            "glob": "*.md",
            "output": "./"
          },
          {
            "input": "./libs/dev-brand/backend/data-access",
            "glob": "package.json",
            "output": "./"
          }
        ]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project tsconfig.lib.json",
        "cwd": "libs/dev-brand/backend/data-access"
      },
      "dependsOn": ["^typecheck"],
      "cache": true
    }
  }
}
