{
  "name": "@hive-academy/nestjs-chromadb",
  "version": "0.0.1",
  "description": "NestJS integration for ChromaDB vector database with comprehensive TypeScript support",
  "author": "Hive Academy Team",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "chromadb",
    "vector-database",
    "embeddings",
    "ai",
    "machine-learning",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hive-academy/nestjs-ai-saas-starter.git",
    "directory": "libs/nestjs-chromadb"
  },
  "homepage": "https://github.com/hive-academy/nestjs-ai-saas-starter#readme",
  "bugs": {
    "url": "https://github.com/hive-academy/nestjs-ai-saas-starter/issues"
  },
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.cjs.js"
    }
  },
  "files": [
    "index.*.js",
    "index.*.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "src/**/*.d.ts",
    "src/**/*.d.ts.map",
    "*.md",
    "!*.tsbuildinfo"
  ],
  "peerDependencies": {
    "@langchain/anthropic": "^0.3.26",
    "@langchain/core": "^0.3.68",
    "@langchain/google-genai": "^0.2.16",
    "@langchain/langgraph": "^0.4.3",
    "@langchain/langgraph-checkpoint": "^0.1.0",
    "@langchain/langgraph-checkpoint-sqlite": "^0.2.0",
    "@langchain/openai": "^0.6.7",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "chromadb": "^3.0.11",
    "cohere-ai": "^7.18.0",
    "langchain": "^0.3.30",
    "neo4j-driver": "^5.28.1",
    "openai": "^5.12.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.2"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.0"
  }
}
