{
  "name": "agentic-memory",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/demo/agentic-memory/src",
  "projectType": "library",
  "tags": ["domain:langgraph", "type:data-access", "module:memory"],
  "// targets": "to see all targets run: nx show project agentic-memory --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/demo/agentic-memory",
        "main": "libs/demo/agentic-memory/src/index.ts",
        "tsConfig": "libs/demo/agentic-memory/tsconfig.lib.json",
        "assets": ["libs/demo/agentic-memory/*.md"]
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/demo/agentic-memory/tsconfig.lib.json"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/demo/agentic-memory/jest.config.ts"
      }
    }
  }
}
