{
  "name": "@hive-academy/langgraph-platform",
  "version": "0.0.1",
  "private": false,
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.esm.js"
    }
  },
  "nx": {
    "name": "langgraph-modules/platform",
    "tags": [
      "scope:langgraph",
      "type:util"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/langgraph-modules/platform",
          "tsConfig": "libs/langgraph-modules/platform/tsconfig.lib.json",
          "packageJson": "libs/langgraph-modules/platform/package.json",
          "main": "libs/langgraph-modules/platform/src/index.ts",
          "assets": [
            "libs/langgraph-modules/platform/*.md"
          ]
        }
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "tslib": "^2.3.0",
    "@nestjs/common": "^11.0.0",
    "axios": "^1.6.0"
  }
}
