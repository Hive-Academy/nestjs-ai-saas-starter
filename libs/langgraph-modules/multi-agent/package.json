{
  "name": "@hive-academy/langgraph-multi-agent",
  "version": "0.0.1",
  "private": false,
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.esm.js"
    }
  },
  "nx": {
    "name": "langgraph-modules/multi-agent",
    "tags": [
      "domain:langgraph",
      "type:feature",
      "module:multi-agent"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/langgraph-modules/multi-agent",
          "tsConfig": "libs/langgraph-modules/multi-agent/tsconfig.lib.json",
          "packageJson": "libs/langgraph-modules/multi-agent/package.json",
          "main": "libs/langgraph-modules/multi-agent/src/index.ts",
          "assets": [
            "libs/langgraph-modules/multi-agent/*.md"
          ]
        }
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nestjs/common": "^11.0.0",
    "@langchain/core": "^0.3.68",
    "@langchain/langgraph": "^0.4.3",
    "reflect-metadata": "^0.1.13",
    "@hive-academy/langgraph-core": "0.0.1",
    "@nestjs-plus/discovery":"2.0.2"
  }
}
