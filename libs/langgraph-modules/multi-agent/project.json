{
  "name": "langgraph-modules/multi-agent",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/langgraph-modules/multi-agent/src",
  "projectType": "library",
  "tags": ["domain:langgraph", "type:feature", "module:multi-agent"],
  "// targets": "to see all targets run: nx show project langgraph-modules/multi-agent --web",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/langgraph-modules/multi-agent",
        "main": "libs/langgraph-modules/multi-agent/src/index.ts",
        "tsConfig": "libs/langgraph-modules/multi-agent/tsconfig.lib.json",
        "rollupConfig": "libs/langgraph-modules/multi-agent/rollup.config.js",
        "format": ["cjs", "esm"],

        "external": ["@hive-academy/langgraph-core"],
        "useLegacyTypescriptPlugin": false,
        "assets": [
          {
            "input": "./libs/langgraph-modules/multi-agent",
            "glob": "*.md",
            "output": "./"
          },
          {
            "input": "./libs/langgraph-modules/multi-agent",
            "glob": "package.json",
            "output": "./"
          }
        ]
      }
    }
  }
}
