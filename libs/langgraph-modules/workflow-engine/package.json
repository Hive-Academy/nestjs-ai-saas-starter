{
  "name": "@hive-academy/langgraph-workflow-engine",
  "version": "0.0.1",
  "private": false,
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.esm.js"
    }
  },
  "files": [
    "index.*.js",
    "index.*.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "src/**/*.d.ts",
    "src/**/*.d.ts.map",
    "*.md",
    "!*.tsbuildinfo"
  ],
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@langchain/core": "^0.3.68",
    "@nestjs/event-emitter": "^3.0.1",
    "rxjs": "7.8.2",
    "@langchain/langgraph": "^0.4.3",
    "reflect-metadata": "^0.1.13",
    "@langchain/langgraph-checkpoint": "^0.1.0",
    "@langchain/langgraph-checkpoint-sqlite": "^0.2.0",
    "@hive-academy/langgraph-streaming": "0.0.1",
    "@hive-academy/langgraph-core": "0.0.1",
    "@hive-academy/langgraph-functional-api": "0.0.1"
  },
  "devDependencies": {
    "rollup": "4.49.0",
    "@rollup/plugin-typescript": "12.1.4",
    "rollup-plugin-dts": "6.2.3"
  }
}
