{
  "name": "langgraph-modules/memory",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "tags": [
    "memory",
    "database",
    "nestjs",
    "domain:langgraph",
    "type:data-access",
    "module:memory"
  ],
  "sourceRoot": "libs/langgraph-modules/memory/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/langgraph-modules/memory",
        "main": "libs/langgraph-modules/memory/src/index.ts",
        "tsConfig": "libs/langgraph-modules/memory/tsconfig.lib.json",
        "rollupConfig": "libs/langgraph-modules/memory/rollup.config.js",
        "format": ["cjs", "esm"],
        "useLegacyTypescriptPlugin": false,
        "assets": [
          {
            "input": "./libs/langgraph-modules/memory",
            "glob": "*.md",
            "output": "./"
          },
          {
            "input": "./libs/langgraph-modules/memory",
            "glob": "package.json",
            "output": "./"
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{options.outputFile}"],
      "options": {
        "jestConfig": "libs/langgraph-modules/memory/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "coverage": true
        }
      }
    }
  }
}
