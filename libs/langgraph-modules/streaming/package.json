{
  "name": "@hive-academy/langgraph-streaming",
  "version": "0.0.1",
  "private": false,
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.esm.js"
    }
  },
  "files": [
    "index.*.js",
    "index.*.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "src/**/*.d.ts",
    "src/**/*.d.ts.map",
    "*.md",
    "!*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "@nestjs/event-emitter": "^3.0.1",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "rollup": "4.49.0",
    "@rollup/plugin-typescript": "12.1.4",
    "rollup-plugin-dts": "6.2.3",
    "@nestjs/testing": "^11.0.0"
  }
}
