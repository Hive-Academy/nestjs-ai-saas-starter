{
  "name": "langgraph-modules/monitoring",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/langgraph-modules/monitoring/src",
  "projectType": "library",
  "tags": ["domain:langgraph", "type:data-access", "module:monitoring"],
  "// targets": "to see all targets run: nx show project langgraph-modules/monitoring --web",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/langgraph-modules/monitoring",
        "main": "libs/langgraph-modules/monitoring/src/index.ts",
        "tsConfig": "libs/langgraph-modules/monitoring/tsconfig.lib.json",
        "rollupConfig": "libs/langgraph-modules/monitoring/rollup.config.js",
        "format": ["cjs", "esm"],
        "useLegacyTypescriptPlugin": false,
        "assets": [
          {
            "input": "./libs/langgraph-modules/monitoring",
            "glob": "*.md",
            "output": "./"
          },
          {
            "input": "./libs/langgraph-modules/monitoring",
            "glob": "package.json",
            "output": "./"
          }
        ]
      }
    }
  }
}
