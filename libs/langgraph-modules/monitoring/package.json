{
  "name": "@hive-academy/langgraph-monitoring",
  "version": "0.0.1",
  "private": false,
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.esm.js"
    }
  },
  "files": [
    "index.*.js",
    "index.*.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "src/**/*.d.ts",
    "src/**/*.d.ts.map",
    "*.md",
    "!*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "reflect-metadata": "^0.1.13"
  }
}
