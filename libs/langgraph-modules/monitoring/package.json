{
  "name": "@hive-academy/langgraph-monitoring",
  "version": "0.0.1",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "langgraph-modules/monitoring",
    "tags": [
      "scope:langgraph",
      "type:util"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/langgraph-modules/monitoring",
          "tsConfig": "libs/langgraph-modules/monitoring/tsconfig.lib.json",
          "packageJson": "libs/langgraph-modules/monitoring/package.json",
          "main": "libs/langgraph-modules/monitoring/src/index.ts",
          "assets": [
            "libs/langgraph-modules/monitoring/*.md"
          ]
        }
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nestjs/common": "^11.0.0"
  }
}
