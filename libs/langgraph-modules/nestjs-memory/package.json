{
  "name": "@hive-academy/nestjs-memory",
  "version": "1.0.0",
  "description": "Standalone NestJS Memory Module with direct ChromaDB and Neo4j integration",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "nestjs",
    "memory",
    "chromadb",
    "neo4j",
    "vector-database",
    "graph-database",
    "semantic-search",
    "ai"
  ],
  "author": "Hive Academy",
  "license": "MIT",
  "nx": {
    "tags": [
      "memory",
      "database",
      "nestjs"
    ],
    "name": "nestjs-memory"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@hive-academy/nestjs-chromadb": "*",
    "@hive-academy/nestjs-neo4j": "*",
    "@langchain/core": "^0.1.0",
    "zod": "^3.22.0",
    "rxjs": "^7.8.0",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "rxjs": "^7.0.0",
    "reflect-metadata": "^0.1.12"
  }
}
