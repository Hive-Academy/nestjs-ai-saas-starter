{
  "name": "@hive-academy/langgraph-checkpoint",
  "version": "0.0.1",
  "private": false,
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.esm.js"
    }
  },
  "files": [
    "index.*.js",
    "index.*.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "src/**/*.d.ts",
    "src/**/*.d.ts.map",
    "*.md",
    "!*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@langchain/langgraph": "^0.4.0",
    "@hive-academy/langgraph-core": "0.0.1"
  },
  "optionalDependencies": {
    "ioredis": "^5.0.0",
    "pg": "^8.0.0",
    "sqlite": "^5.0.0",
    "sqlite3": "^5.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
