{
  "name": "langgraph-modules/hitl",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "tags": [
    "domain:langgraph",
    "type:data-access",
    "module:checkpoint"
  ],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/langgraph-modules/hitl",
        "main": "libs/langgraph-modules/hitl/src/index.ts",
        "tsConfig": "libs/langgraph-modules/hitl/tsconfig.lib.json",
        "format": ["cjs", "esm"],
        "buildLibsFromSource": true,
        "external": ["@hive-academy/langgraph-core"],
        "useLegacyTypescriptPlugin": false,
        "assets": [
          {
            "input": "./libs/langgraph-modules/hitl",
            "glob": "*.md",
            "output": "./"
          },
          {
            "input": "./libs/langgraph-modules/hitl",
            "glob": "package.json",
            "output": "./"
          }
        ]
      }
    }
  },
  "sourceRoot": "libs/langgraph-modules/hitl/src",
  "projectType": "library"
}
