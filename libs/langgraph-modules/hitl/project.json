{
  "name": "langgraph-modules/hitl",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "tags": [
    "domain:langgraph",
    "type:data-access",
    "module:checkpoint"
  ],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/langgraph-modules/hitl",
        "main": "libs/langgraph-modules/hitl/src/index.ts",
        "tsConfig": "libs/langgraph-modules/hitl/tsconfig.lib.json",
        "rollupConfig": "libs/langgraph-modules/hitl/rollup.config.js",
        "format": [
          "cjs",
          "esm"
        ],
        "useLegacyTypescriptPlugin": false,
        "external": [
          "@hive-academy/langgraph-core"
        ],
        "assets": [
          {
            "input": "./libs/langgraph-modules/hitl",
            "glob": "*.md",
            "output": "./"
          },
          {
            "input": "./libs/langgraph-modules/hitl",
            "glob": "package.json",
            "output": "./"
          }
        ]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project tsconfig.lib.json",
        "cwd": "libs/langgraph-modules/hitl"
      },
      "dependsOn": [
        "^typecheck"
      ],
      "cache": true
    }
  },
  "sourceRoot": "libs/langgraph-modules/hitl/src",
  "projectType": "library"
}
